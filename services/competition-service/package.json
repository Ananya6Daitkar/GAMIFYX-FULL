{
  "name": "competition-service",
  "version": "1.0.0",
  "description": "External competition integration service for GamifyX platform",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "ts-node-dev --respawn --transpile-only -r tsconfig-paths/register src/index.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "migrate": "ts-node -r tsconfig-paths/register src/database/migrator.ts migrate",
    "migrate:status": "ts-node -r tsconfig-paths/register src/database/migrator.ts status",
    "migrate:rollback": "ts-node -r tsconfig-paths/register src/database/migrator.ts rollback",
    "test:models": "ts-node -r tsconfig-paths/register src/database/test-models.ts",
    "test:competition-manager": "ts-node -r tsconfig-paths/register src/services/test-competition-manager.ts",
    "test:external-api": "ts-node -r tsconfig-paths/register src/services/test-external-api.ts",
    "test:notifications": "ts-node -r tsconfig-paths/register src/services/test-notification-system.ts"
  },
  "dependencies": {
    "@opentelemetry/api": "^1.4.1",
    "@opentelemetry/auto-instrumentations-node": "^0.39.4",
    "@opentelemetry/exporter-jaeger": "^1.15.2",
    "@opentelemetry/exporter-prometheus": "^0.41.2",
    "@opentelemetry/sdk-node": "^0.41.2",
    "@octokit/rest": "^20.0.2",
    "axios": "^1.6.0",
    "cors": "^2.8.5",
    "cron": "^3.1.6",
    "express": "^4.18.2",
    "express-rate-limit": "^6.8.1",
    "helmet": "^7.0.0",
    "joi": "^17.9.2",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "node-cron": "^3.0.3",
    "nodemailer": "^6.9.7",
    "pg": "^8.11.3",
    "redis": "^4.6.7",
    "socket.io": "^4.7.2",
    "uuid": "^9.0.0",
    "winston": "^3.10.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.13",
    "@types/cron": "^2.0.1",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.3",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/morgan": "^1.9.4",
    "@types/node": "^20.4.2",
    "@types/node-cron": "^3.0.8",
    "@types/nodemailer": "^6.4.14",
    "@types/pg": "^8.10.2",
    "@types/supertest": "^2.0.12",
    "@types/uuid": "^9.0.2",
    "@typescript-eslint/eslint-plugin": "^6.2.0",
    "@typescript-eslint/parser": "^6.2.0",
    "eslint": "^8.45.0",
    "jest": "^29.6.1",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.6"
  },
  "keywords": [
    "competition",
    "gamification",
    "hacktoberfest",
    "github",
    "gitlab",
    "microservice"
  ],
  "author": "GamifyX Platform",
  "license": "MIT"
}