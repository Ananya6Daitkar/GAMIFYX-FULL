    "@opentelemetry/sdk-node": "^0.41.2",
    "axios": "^1.5.0",
    "cors": "^2.8.5",
    "cron": "^3.1.6",
    "express": "^4.18.2",
    "express-rate-limit": "^6.8.1",
    "helmet": "^7.0.0",
    "joi": "^17.9.2",
    "jsonwebtoken": "^9.0.2",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "morgan": "^1.10.0",
